{
  "name": "Emporix Connect for Support",
  "author": {
    "name": "neteleven",
    "email": "zendesk@neteleven.de",
    "url": "https://www.neteleven.de/"
  },
  "defaultLocale": "en",
  "private": false,
  "location": {
    "support": {
      "ticket_sidebar": {
        "url": "assets/iframe.html",
        "flexible": true
      }
    }
  },
  "domainWhitelist": [
    "api.emporix.io"
  ],
  "parameters": [
    {
      "name": "clientId",
      "type": "text",
      "required": true,
      "secure": true
    },
    {
      "name": "clientSecret",
      "type": "text",
      "required": true,
      "secure": true
    },
    {
      "name": "tenant",
      "type": "text",
      "required": true,
      "secure": false
    },
    {
      "name": "orderDisplayLimit",
      "type": "number",
      "default": 3,
      "secure": false
    }
  ],
  "version": "1.0",
  "frameworkVersion": "2.0"
}
